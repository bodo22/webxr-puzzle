    "version": "0.2.0",
    "configurations": [
        {
            "type": "chrome",
            "request": "attach",
            "name": "Attach to local 9222 port",
            "url": "https://localhost:3000",
            "webRoot": "${workspaceFolder}",
            "urlFilter": "https://localhost:3000/*",
            "port": 9222
        },
        {
            "type": "android-webview",
            "request": "attach",
            "name": "Attach to Android WebView",
            "application": "com.android.chrome",
            "device": "1WMHHA63U21426"
        }
    ]
}